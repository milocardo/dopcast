<div class="carousel">
  <div class="contain">
    <div class="scroll-row">
      <div class="row__inner">
        <% @episodes["results"].each do |episode| %>
          <div class="tile">
            <div class="tile__media">
              <div><%=link_to( image_tag(episode["image"]), podcast_path(episode["id"]), :class=>"img-size") %></div>
            </div>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
