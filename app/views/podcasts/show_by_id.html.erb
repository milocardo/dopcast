<div class="podcard-show">
  <div class="row">
    <div class="col-xs-12 col-sm-6 col-md-6">
      <div class="podcard">
        <%= image_tag @podcast_id['results'][0]["artworkUrl600"], class: "img-responsive" %>
      </div>
    </div>
    <div class="col-xs-12 col-sm-6 col-md-6">
      <h1><%= @podcast_id['results'][0]["artistName"] %></h1>
      <h2><%= @podcast_id['results'][0]["collectionName"] %></h2>
    </div>
  </div>
</div>
