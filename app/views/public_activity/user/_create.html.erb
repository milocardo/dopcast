<div class="container-text">

<li class="list-group-item">
  <span class="glyphicon glyphicon-user"></span>
  <small class="text-muted"><%= a.created_at.strftime('%H:%M:%S %-d %B %Y') %></small>
  <% if a.trackable %>
    <%= link_to a.trackable.username, user_path(a.trackable) %> joined Dopcast
  <% else %>
    A user that is currently deleted was added.
  <% end %>
</li>

</div>
